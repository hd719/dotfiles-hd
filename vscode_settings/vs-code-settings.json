{
  "editor.tabSize": 2,
  "git.enableSmartCommit": true,
  "window.zoomLevel": -1,
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "typescript.check.npmIsInstalled": false,
  "workbench.startupEditor": "newUntitledFile",
  "emmet.triggerExpansionOnTab": true,
  "workbench.colorCustomizations": {
    //
    "activityBar.background": "#0e141f",
    "activityBar.foreground": "#ffffff",
    "activityBar.border": "#ffffff",
    //
    "statusBar.background": "#000000",
    "statusBar.foreground": "#ffffff",
    //
    "tab.activeForeground": "#ffffff",
    "tab.activeBackground": "#525050",
    "tab.inactiveBackground": "#0e141f",
    "tab.inactiveForeground": "#ffffff",
    //
    "editor.background": "#0e141f",
    //"editor.foreground": "#0e141f",
    "editor.lineHighlightBackground": "#3d484b",
    "editor.selectionBackground": "#3d484b",
    "editorGroupHeader.tabsBackground": "#0e141f",
    "editorGroupHeader.tabsBorder": "#ffffff",
    "editorGroup.border": "#ffffff",
    "editorLineNumber.foreground": "#c8c8c8",
    //
    "sideBar.border": "#ffffff",
    "sideBar.background": "#0e141f",
    //
    "panel.background": "#0e141f",
    "terminal.background": "#0e141f",
    "titleBar.activeBackground": "#1A1A1A"
  },
  "workbench.panel.location": "right",
  "editor.fontSize": 12,
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": true,
  "workbench.sideBar.location": "left",
  "editor.minimap.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": null,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "explorer.confirmDragAndDrop": false,
  "flow.useNPMPackagedFlow": true,
  "javascript.validate.enable": false,
  "explorer.confirmDelete": false,
  "editor.formatOnSave": true,
  "emmet.syntaxProfiles": {
    "javascript": "html",
    "javascript": "jsx"
  },
  "editor.renderIndentGuides": false,
  "workbench.colorTheme": "Snazzy Operator"
