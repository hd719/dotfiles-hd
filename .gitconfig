[user]
	name = Hamel Desai
	email = hameldesai93@gmail.com
[core]
	editor = vim
[web]
	browser = google-chrome
[alias]
  co = checkout
  br = branch
	bdd = branch -D
  ci = commit
	cm = commit -m
	cem = commit --allow-empty -m
  st = status
[color]
  ui = true
[color "branch"]
	current = yellow bold
	local = green bold
	remote = cyan bold
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red bold